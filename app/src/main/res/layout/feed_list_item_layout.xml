<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:id="@+id/feed_list_item_layout"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="@dimen/feed_list_item_height"
    android:minHeight="@dimen/feed_list_item_height">

    <TextView
        android:id="@+id/feed_list_item_text_view_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"/>

    <TextView
        android:id="@+id/feed_list_item_text_view_url"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"/>

</LinearLayout>